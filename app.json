{
  "expo": {
    "name": "ASOS",
    "description": "A React-Native Clone for the popular online shopping company - ASOS.",
    "slug": "asos",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.co.edwinb.ASOS",
      "buildNumber": "001"
    },
    "android": {
      "package": "uk.co.edwinb.ASOS"
    },
    "githubUrl": "https://github.com/edwinbosire/ASOS"
  }
}
